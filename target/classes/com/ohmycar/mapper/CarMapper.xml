	<select id="getCarByUserId"
		resultType="com.ohmycar.domain.CarVO">
		SELECT * FROM car_tbl WHERE userid = #{userId}
	</select>




	<update id="updateCar" parameterType="com.ohmycar.domain.CarVO">
		UPDATE car_tbl
		SET
		carid = #{carId},
		carsellname = #{carSellName},
		carname = #{carName},
		cartype = #{carType}
		WHERE userid= #{userId}
	</update>


</mapper>
