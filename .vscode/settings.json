{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".project": true,
        "log4j.xml": true,
        "**/log4j.xml": true,
        "**/.project": true
    },
    "explorerExclude.backup": {},
    "java.configuration.updateBuildConfiguration": "automatic",
    "cSpell.words": [
        "jsonschema",
        "ohmycar"
    ],
    "java.compile.nullAnalysis.mode": "automatic",
    "java.debug.settings.onBuildFailureProceed": true
}